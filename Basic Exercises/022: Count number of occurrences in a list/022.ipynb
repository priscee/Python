   "source": [
    "Write a  Python program to count the number 4 in a given list."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def list_count(nums):\n",
    "\n",
    "    count = 0\n",
    "\n",
    "    for num in nums:\n",
    "        if num == 4:\n",
    "            count = count + 1\n",
    "    return count\n",
    "\n",
    "print(list_count([1,2,4,6,8,4]))\n",
    "print(list_count([1,3,6,7,8,9,]))"
   ]
  }
