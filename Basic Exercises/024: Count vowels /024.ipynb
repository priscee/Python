   "source": [
    "Write a Python program to test whether a passed letter is a vowel or not."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "letter = str(input(\"Your letter: \"))\n",
    "\n",
    "def is_vowel(letter):\n",
    "    all_vowels = 'aeiou'\n",
    "\n",
    "    return letter in all_vowels\n",
    "\n",
    "print(is_vowel(letter))"
   ]
  }
